var food = ["Sushi", "Curry", "Steak", "Spaghetti", "Pizza"];



var changeView = function(){
	var window = Ti.UI.createWindow({
		backgroundColor: "#fff"
	});
	buttonView1 = Ti.UI.createView({
		height: 50,
		width: 75,
		left: 20,
		borderRadius: 5,
		top: 330,
		color: "blue",
		text: "previous"
		textColor: "white"
	});
	var buttonView2 = Ti.UI.createView({
		height: 50,
		width: 75,
		right: 20,
		borderRadius: 5,
		top: 330,
		color: "blue",
		text: "next",
		textColor: "white"
	});
};


buttonView.addEventListener("click", changeView);





