var preButton = Ti.UI.createView({
	height: 50,
	width: 75,
    left: 60,
    borderRadius: 5,
    top: 290,
    backgroundColor: "#2A53E1"
});
var nexButton = Ti.UI.createView({
	height: 50,
    width: 75,
    right: 60,
    borderRadius: 5,
    top: 290,
    backgroundColor: "#2A53E1"
});

var adding = function(
	window.add(preButton),
	window.add(nexButton),
);
//var adding = window.add(changeButton);

buttonView.addEventListener("click", adding);


